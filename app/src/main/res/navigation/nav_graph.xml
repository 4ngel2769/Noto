<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/libraryListFragment">
    <fragment
        android:id="@+id/libraryListFragment"
        android:name="com.noto.library.LibraryListFragment"
        android:label="LibraryListFragment"
        tools:layout="@layout/fragment_list_library">
        <action
            android:id="@+id/action_libraryListFragment_to_libraryFragment"
            app:destination="@id/libraryFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/action_libraryListFragment_to_labelListFragment"
            app:destination="@id/labelListFragment" />
    </fragment>
    <fragment
        android:id="@+id/libraryFragment"
        android:name="com.noto.library.LibraryFragment"
        android:label="LibraryFragment"
        tools:layout="@layout/fragment_library">
        <argument
            android:name="library_id"
            android:defaultValue="0L"
            app:argType="long" />
        <argument
            android:name="notoColor"
            app:argType="com.noto.domain.model.NotoColor" />
        <action
            android:id="@+id/action_libraryFragment_to_notoFragment"
            app:destination="@id/notoFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/action_libraryFragment_to_notoNewFragment"
            app:destination="@id/notoNewFragment" />
    </fragment>
    <argument
        android:name="library_id"
        app:argType="long" />
    <argument
        android:name="noto_id"
        app:argType="long" />
    <fragment
        android:id="@+id/notoFragment"
        android:name="com.noto.noto.NotoFragment"
        android:label="NotoFragment"
        tools:layout="@layout/fragment_noto">
        <argument
            android:name="library_id"
            app:argType="long" />
        <argument
            android:name="noto_id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/labelListFragment"
        android:name="com.noto.label.LabelListFragment"
        android:label="LabelListFragment"
        tools:layout="@layout/fragment_list_label" />
    <fragment
        android:id="@+id/notoNewFragment"
        android:name="com.noto.noto.NotoNewFragment"
        android:label="NotoNewFragment" />
</navigation>